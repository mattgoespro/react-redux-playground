{
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true
  },
  "auto-close-tag.activationOnLanguage": ["html", "javascriptreact", "typescriptreact", "markdown"],
  "auto-close-tag.disableOnLanguage": ["javascript", "typescript", "plaintext"],
  "code-runner.cwd": "${workspaceFolder}",
  "code-runner.defaultLanguage": "TypeScript",
  "code-runner.executorMap": {
    "javascript": "node",
    "typescript": "ts-node"
  },
  "code-runner.fileDirectoryAsCwd": true,
  "css.format.enable": false,
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": true,
    "source.fixAll.scss": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.codeActionsOnSave.mode": "all",
  "eslint.enable": true,
  "eslint.format.enable": false,
  "eslint.probe": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "eslint.run": "onType",
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "explorer.confirmDragAndDrop": false,
  "files.exclude": {
    ".git": true,
    "dll": true,
    "**/node_modules/**": true,
    "release/app/{dist,build,node_modules}": true
  },
  "files.watcherExclude": {
    ".git": true,
    "dll": true,
    "**/node_modules/**": true,
    "release/app/{dist,build,node_modules}": true,
    "resources": true
  },
  "html.format.enable": false,
  "javascript.format.enable": false,
  "javascript.suggestionActions.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.validate.enable": false,
  "markdown.extension.preview.autoShowPreviewToSide": true,
  "markdown.occurrencesHighlight.enabled": true,
  "material-icon-theme.activeIconPack": "react",
  "material-icon-theme.files.color": "#000000",
  "material-icon-theme.folders.color": "#eb7734",
  "material-icon-theme.hidesExplorerArrows": true,
  "npm.exclude": "release/**",
  "prettier.configPath": ".prettierrc.js",
  "react-native-tools.networkInspector.consoleLogsColorTheme": "Dark",
  "react-native-tools.projectRoot": "${workspaceFolder}/src/renderer",
  "scss.completion.completePropertyWithSemicolon": true,
  "scss.completion.triggerPropertyValueCompletion": true,
  "scss.format.enable": false,
  "scss.showErrors": true,
  "scssFormatter.printWidth": 100,
  "scssFormatter.singleQuote": false,
  "search.exclude": {
    "*.{css,sass,scss}.d.ts": true,
    ".git": true,
    "dist": true,
    "dll": true,
    "node_modules": true,
    "npm-debug.log.*": true,
    "package-lock.json": true,
    "release/{build,app/dist}": true,
    "resources": true
  },
  "typescript.format.enable": false,
  "typescript.tsdk": "node_modules\\typescript\\lib",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.iconTheme": "material-icon-theme",
  "auto-close-tag.enableAutoCloseTag": false,
  "auto-close-tag.SublimeText3Mode": true
}
